@{
    Layout = "~/Views/Shared/_LayoutViewDiclarge.cshtml";
}
<script>

    function cmpDialogDic_DG_init() {
        $('#cmpDialogDic_DG').datagrid({
                singleSelect: true,
                pagination: true,
                rownumbers: true,
                remoteSort: true,
                height: "450",
                width: "800",
                toolbar: "#toolbarDialogDic_DG",
                @* __TIPS__ *: 以下同表格中显示的每列的配置信息 *@
                columns: [[
                { field: 'DM', title: '制证员账户', width: 200 },
                { field: 'MC', title: '制证员名称', width: 200 },
                { field: 'DW', title: '制证员所在单位', width: 210 },
            ]]
        });
    }
</script>
